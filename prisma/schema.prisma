generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Menu {
  id         String @id @default(uuid())
  title       String
  image      String
  price      Float
  category   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("menu")
}